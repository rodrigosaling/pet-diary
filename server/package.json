{
  "name": "pet-diary-server",
  "private": true,
  "version": "0.0.1",
  "description": "The backend side of Pet Diary.",
  "author": "Rodrigo Saling",
  "license": "MIT",
  "homepage": "https://github.com/rodrigosaling/pet-diary/server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigosaling/pet-diary.git"
  },
  "bugs": {
    "url": "https://github.com/rodrigosaling/pet-diary/issues"
  },
  "type": "module",
  "main": "./src/index.js",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "dev:watch": "tsx watch ./src/index.ts",
    "build": "tsup ./src/index.ts --format esm",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "knex": "knex",
    "reset-db": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "start": "node ./src/index.js"
  },
  "keywords": [
    "nodejs",
    "express"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "tsup": "^6.5.0",
    "tsx": "^3.12.2",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^2.4.0",
    "sqlite3": "^5.1.4",
    "zod": "^3.20.2"
  }
}
